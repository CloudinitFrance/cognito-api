{
  "definitions": {}, 
  "$schema": "http://json-schema.org/draft-07/schema#", 
  "type": "object", 
  "title": "Set user new password Schema", 
  "required": [
    "email", 
    "new_password",
    "verification_code"
  ], 
  "properties": {
    "email": {
      "$id": "#/properties/email", 
      "type": "string", 
      "title": "The user name", 
      "examples": [
        "tarek@lostinmac.com"
      ]
    },
    "new_password": {
      "$id": "#/properties/new_password", 
      "type": "string", 
      "title": "The user password", 
      "examples": [
        "Il0veThis!Password?"
      ]
    },
    "verification_code": {
      "$id": "#/properties/verification_code", 
      "type": "string", 
      "title": "The verification code",
      "examples": [
        "728243"
      ]
    }
  },
  "additionalProperties": false
}
