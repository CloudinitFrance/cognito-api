{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Change User Password Schema",
  "required": [
    "email",
    "old_password",
    "new_password",
    "access_token"
  ],
  "properties": {
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The user email",
      "examples": [
        "my-email@mail42.com"
      ]
    },
    "old_password": {
      "$id": "#/properties/old_password",
      "type": "string",
      "title": "The user old password",
      "examples": [
        "myOldOrCurrentPassW0rd"
      ]
    },
    "new_password": {
      "$id": "#/properties/new_password",
      "type": "string",
      "title": "The user new password",
      "examples": [
        "mySecureNewPassw0Rd"
      ]
    },
    "access_token": {
      "$id": "#/properties/access_token",
      "type": "string",
      "title": "The user access token",
      "examples": [
        "eyJraWQiOiJTdEhEc2Z2dHdIT0RHaTF3dDM4bVkxZjlSb1BVYmFlOHdDNUsxNGNSRW9vPSIsImFsZyI6IlJTMjU2In0."
      ]
    }
  },
  "additionalProperties": false
}
